<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :super_name %>
<%= f.text_field :super_name %>
<%= f.collection_select :power_id, Power.all, :id, :name %>
<%= f.submit %>
<% end %>


<%= form_tag("/heroines/#{@heroine.id}", method: "delete") do %>
<%= submit_tag("DELETE heroine") %>
<% end %>